<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.sun.javafx.scene.control.skin.TableHeaderRow?>
<?import com.jfoenix.controls.JFXListView?>
<?import com.jfoenix.controls.JFXButton?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.rmt.gui.controllers.MatchMakingController"
          alignment="CENTER" gridLinesVisible="true"
          stylesheets="@../style/test-theme.css"
          prefHeight="400.0" prefWidth="600.0">

    <columnConstraints>
        <ColumnConstraints percentWidth="40" halignment="CENTER"/>
    </columnConstraints>

    <rowConstraints>
        <RowConstraints percentHeight="60" valignment="CENTER"/>
        <RowConstraints percentHeight="10" valignment="CENTER"/>
        <RowConstraints percentHeight="10" valignment="CENTER"/>
    </rowConstraints>

    <JFXListView fx:id = "activePlayersListView" GridPane.rowIndex="0" GridPane.columnIndex="0" >

    </JFXListView>


    <JFXButton fx:id="refreshButton" onAction="#onRefreshButtonClicked" text="REFRESH" styleClass="start-button"
               GridPane.rowIndex="1" GridPane.columnIndex="0"/>

    <JFXButton fx:id="logOutButton" onAction="#onLogoutButtonClicked" text="LOG OUT" styleClass="start-button"
               GridPane.rowIndex="2" GridPane.columnIndex="0"/>


</GridPane>
